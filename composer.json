{
    "name": "lohiaad/laravel-phumbor",
    "description": "Laravel package adding support for the Phumbor PHP library for Thumbor.",
	"homepage": "https://github.com/lohiaad/laravel-phumbor",
	"license": "MIT",
	"keywords": ["laravel", "phumbor", "thumbor"],
    "authors": [
        {
            "name": "Chris Sternal-Johnson",
            "email": "chris@sternal-johnson.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": ">=5.0",
        "99designs/phumbor": "^1.2"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Ceejayoz\\LaravelPhumbor": "src/"
        }
    },
    "minimum-stability": "dev"
}
